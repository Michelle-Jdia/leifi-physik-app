    "dotenv": "^16.3.1",
    "ionicons": "^7.1.2",
    "localforage-cordovasqlitedriver": "^1.8.0",
    "mathjax": "3.2.2",
    "pinia": "^2.1.6",
    "ramda": "^0.29.0",
    "swiper": "^10.2.0",
    "tailwind-merge": "^1.14.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.4.0",
